@model IEnumerable<MegaMenu.Models.Categories>

@if (Model.Count() > 0)
{
    <ul>
        @foreach (var item in Model)
        {
        <li>
            <a href="#" title="Trang chủ">@item.Name</a>
            @Html.Action("LoadChild", "Home", new { parentId = item.Id })
        </li>
        }
    </ul>
}
